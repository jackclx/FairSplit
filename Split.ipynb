    "    # Get the dispatcher to register handlers\n",
    "    dp = updater.dispatcher\n",
    "\n",
    "    # Define conversation handler\n",
    "    conv_handler = ConversationHandler(\n",
    "        entry_points=[CommandHandler('addexpense', add_expense)],\n",
    "        states={\n",
